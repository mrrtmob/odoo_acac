<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="_assets_helpers" name="_assets_helpers ( Backend Theme Assets )" inherit_id="web._assets_primary_variables">
        <xpath expr="." position="inside">
            <link rel="stylesheet" type="text/scss" href='/openeducat_backend_theme/static/src/scss/variables.scss'/>
        </xpath>
    </template>
    <template id="assets_backend" name="assets_backend ( Backend Theme Assets )" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <meta name="viewport" content="width=device-width, initial-scale=1" />
            <link rel="stylesheet" type="text/scss" href='/openeducat_backend_theme/static/src/scss/navbar.scss' />
            <link rel="stylesheet" type="text/scss" href='/openeducat_backend_theme/static/src/scss/apps_menu.scss' />
            <link rel="stylesheet" type="text/scss" href='/openeducat_backend_theme/static/src/scss/fields_extra.scss' />
            <link rel="stylesheet" type="text/scss" href='/openeducat_backend_theme/static/src/scss/search_view_extra.scss' />
            <link rel="stylesheet" type="text/scss" href='/openeducat_backend_theme/static/src/scss/list_view_extra.scss' />
            <link rel="stylesheet" type="text/scss" href='/openeducat_backend_theme/static/src/scss/form_view_extra.scss' />
            <link rel="stylesheet" type="text/scss" href='/openeducat_backend_theme/static/src/scss/webclient_extra.scss' />
            <link rel="stylesheet" type="text/scss" href='/openeducat_backend_theme/static/src/scss/common_style.scss' />

            <script type="text/javascript" src="/openeducat_backend_theme/static/src/js/web_client.js" />
            <script type="text/javascript" src="/openeducat_backend_theme/static/src/js/apps_menu.js" />
            <script type="text/javascript" src="/openeducat_backend_theme/static/src/js/menu.js" />
            <script type="text/javascript" src="/openeducat_backend_theme/static/src/js/systray_activity_menu.js" />
        </xpath>
    </template>
</odoo>
