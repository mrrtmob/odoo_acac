<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="assets_frontend_student_progress" name="Student Progress frontend assets"
                  inherit_id="website.assets_frontend">
            <xpath expr="." position="inside">
                <link type="text/css"
                      href="/openeducat_student_progress_enterprise/static/src/scss/student_progress.scss"
                      rel="stylesheet"/>
            </xpath>
        </template>
        <template id="assets_tests" name="Website Student Progression Assets Tests" inherit_id="web.assets_tests">
            <xpath expr="." position="inside">
                <script type="text/javascript"
                        src="/openeducat_student_progress_enterprise/static/tests/tours/student_progression_test.js"/>
            </xpath>
        </template>
    </data>
</odoo>
