                name="Report"
                groups="pm_culinary.group_acac_culinary,pm_culinary.group_acac_culinary_head"
                sequence="6"
                parent="menu_root" />

        <menuitem id="menu_report_recipe_schedule"
                  name="Recipe Schedule"
                  parent="menu_report"
                  groups="pm_culinary.group_acac_culinary,pm_culinary.group_acac_culinary_head"
                  sequence="1"
                  action="wizard_recipe_schedule_action"/>

      <menuitem id="menu_config"
                name="Configuration"
                groups="pm_culinary.group_acac_culinary,pm_culinary.group_acac_culinary_head"
                sequence="7"
                parent="menu_root" />

        <menuitem id="menu_ingredient_config_group"
                  name="Ingredient"
                  parent="menu_config"
                  groups="pm_culinary.group_acac_culinary,pm_culinary.group_acac_culinary_head"
