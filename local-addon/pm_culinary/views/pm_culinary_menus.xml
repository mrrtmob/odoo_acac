                  parent="pm_culinary.menu_root"
                  sequence="0"
                  action="act_culinary_dashboard"/>

      <menuitem id="menu_schedule"
                parent="menu_root"
                groups="pm_culinary.group_acac_culinary,pm_culinary.group_acac_culinary_head"
                sequence="1"
                action="schedule_action" />

      <menuitem id="menu_menu"
                parent="menu_root"
                groups="pm_culinary.group_acac_culinary,pm_culinary.group_acac_culinary_head"
                sequence="2"
                action="menu_action" />

      <menuitem id="menu_recipe"
                parent="menu_root"
                groups="pm_culinary.group_acac_culinary,pm_culinary.group_acac_culinary_head"
